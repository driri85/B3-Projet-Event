{
  "source": {
    "include": ["./src"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(node_modules/|docs)"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "template": "./node_modules/ink-docstrap/template"
  },
  "templates": {
    "systemName": "Application Evenements B3",
    "footer": "Documentation generated by JSDoc & ink-docstrap",
    "navType": "vertical",
    "theme": "flatly",
    "linenums": true,
    "collapseSymbols": false,
    "inverseNav": false
  }
}